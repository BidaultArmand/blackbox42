{
  "name": "ai-naming-reviewer",
  "version": "1.0.0",
  "description": "AI-powered GitHub Action for intelligent code naming review and auto-fixing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "review": "node --loader ts-node/esm src/github/review.ts",
    "autofix": "node --loader ts-node/esm src/github/autofix.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "github-action",
    "ai",
    "code-review",
    "naming",
    "refactoring"
  ],
  "author": "AI Naming Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks-types": "^7.3.2",
    "openai": "^4.28.0",
    "ts-morph": "^21.0.1",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1",
    "minimatch": "^9.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
